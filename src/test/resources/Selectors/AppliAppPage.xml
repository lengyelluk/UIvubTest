<page xmlns="http://lengyel.com/Framework" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://lengyel.com/Framework http://lengyel.com/Framework/FrameworkSelectors.xsd">
    <GuiElement>
        <name>amountHeader</name>
        <kind>button</kind>
        <identifier>css</identifier>
        <identValue>th#amount</identValue>
    </GuiElement>
    <GuiElement>
        <name>amountValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//tr/td[contains(@class, 'text-right')]/span</identValue>
    </GuiElement>
    <GuiElement>
        <name>statusValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[@class='nowrap']//span[2]</identValue>
    </GuiElement>
    <GuiElement>
        <name>statusIcons</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[@class='nowrap']//span[1]</identValue>
    </GuiElement>
    <GuiElement>
        <name>dateValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[not(@class)]//span[1]</identValue>
    </GuiElement>
    <GuiElement>
        <name>timeValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[not(@class)]//span[contains(@class, 'smaller')]</identValue>
    </GuiElement>
    <GuiElement>
        <name>companyLogos</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[@class='cell-with-media']//img</identValue>
    </GuiElement>
    <GuiElement>
        <name>companyValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[@class='cell-with-media']//span</identValue>
    </GuiElement>
    <GuiElement>
        <name>categoryValues</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//td[@class='text-center']//a</identValue>
    </GuiElement>
    <GuiElement>
        <name>compareExpenses</name>
        <kind>button</kind>
        <identifier>css</identifier>
        <identValue>a#showExpensesChart</identValue>
    </GuiElement>
    <GuiElement>
        <name>flashSaleAdds</name>
        <kind>label</kind>
        <identifier>xpath</identifier>
        <identValue>//img[contains(@src, '.gif')]</identValue>
    </GuiElement>
</page>